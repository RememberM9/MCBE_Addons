{
	"format_version": "1.18.20",
	"minecraft:entity": {
	  "description": {
		"identifier": "minecraft:player",
		"is_spawnable": false,
		"is_summonable": false,
		"is_experimental": false,
		"runtime_identifier": "minecraft:evocation_fang",
			  "scripts": {
				  "animate": [
					  "kcraft_execute_commands"
				  ]
			  },
			  "animations": {
				  "kcraft_execute_commands": "controller.animation.kcraft_player.watch"
			  }
	},
	"component_groups": {
		"kcraft:execute_no_command": {
			"minecraft:skin_id": {
				"value": 0
			},
			"minecraft:timer":{
				"time": 1.00,
				"looping": true,
				"time_down_event":  {	
					"event": "kcraft:remove_skin_ids",
					"target": "self"
				}
			}
		},
		"kcraft:execute_command_id_1": {
			"minecraft:skin_id": {
				"value": 1
			}
		},
		"minecraft:add_bad_omen": {
		  "minecraft:spell_effects": {
			"add_effects": [
			  {
				"effect": "bad_omen",
				"duration": 6000,
				"visible": false,
				"display_on_screen_animation": true
			  }
			]
		  },
		  "minecraft:timer": {
			"time": [ 0.0, 0.0 ],
			"looping": false,
			"time_down_event": {
			  "event": "minecraft:clear_add_bad_omen",
			  "target": "self"
			}
		  }
		},
		"kcraft_raid_trigger_filter": {
			"minecraft:timer": {
				"time": [0.00,0.10],
				"looping": false,
				"time_down_event": {
					"filters": {
						"test": "has_tag",
						"subject": "self",
						"operator": "!=",
						"value": "01"
					},
					"event": "minecraft:trigger_raid"
				}
			}
		},
		"minecraft:clear_bad_omen_spell_effect": {
		  "minecraft:spell_effects": {
		  }
		},
		"minecraft:raid_trigger": {
		  "minecraft:raid_trigger": {
			"triggered_event": {
			  "event": "minecraft:remove_raid_trigger",
			  "target": "self"
			}
		  },
		  "minecraft:spell_effects": {
			"remove_effects": "bad_omen"
		  }
		},
		"immunity":{
			"minecraft:spell_effects": {
				"add_effects": [
					{
						"effect": "fire_resistance",
						"duration": 0.5,
						"amplifier": 255,
						"visible": false,
						"display_on_screen_animation": false
					},
					{
						"effect": "resistance",
						"duration": 0.5,
						"amplifier": 255,
						"visible": false,
						"display_on_screen_animation": false
					}
				],
				"remove_effects": [
					"slowness",
					"nausea",
					"blindness",
					"weakness",
					"poison",
					"wither",
					"fatal_poison"
				]
			},
			"minecraft:timer": {
				"time": [ 0, 0 ],
				"looping": false,
				"time_down_event": {
					"event": "kcraft:remove_immunity",
					"target": "self"
				}
			}
		},
		"kcraft_nerf":{
			"minecraft:spell_effects": {
				"add_effects": [
					{
						"effect": "weakness",
						"duration": 0.5,
						"amplifier": 255,
						"visible": false,
						"display_on_screen_animation": false
					}
				]
			},
			"minecraft:timer": {
				"time": [ 0, 0 ],
				"looping": false,
				"time_down_event": {	
					"event": "kcraft:remove_nerf",
					"target": "self"
				}
			}
		}
	},
	"components": {
		"minecraft:experience_reward": {
		  "on_death": "Math.Min(query.player_level * 7, 100)"
		},
		"minecraft:type_family": {
		  "family": [ "player" ]
		},
		"minecraft:is_hidden_when_invisible": {
		},
		"minecraft:loot": {
		  "table": "loot_tables/empty.json"
		},
		"minecraft:collision_box": {
		  "width": 0.6,
		  "height": 1.8
		},
		"minecraft:can_climb": {
		},
		"minecraft:movement": {
		  "value": 0.1
		},
		"minecraft:hurt_on_condition": {
		  "damage_conditions": [
			{
			  "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
			  "cause": "lava",
			  "damage_per_tick": 4
			}
		  ]
		},
		"minecraft:attack": {
			"damage": 1
		},
		"minecraft:exhaustion_values": {
		  "heal": 6,
		  "jump": 0.05,
		  "sprint_jump": 0.2,
		  "mine": 0.005,
		  "attack": 0.1,
		  "damage": 0.1,
		  "walk": 0.0,
		  "sprint": 0.01,
		  "swim": 0.01
		},
		"minecraft:player.saturation": {
		  "value": 5,
		   "max": 20
		},
		"minecraft:player.exhaustion": {
		  "value": 0,
		  "max": 20
		},
		"minecraft:player.level": {
		  "value": 0,
		  "max": 24791
		},
		"minecraft:player.experience": {
		  "value": 0,
		  "max": 1
		},
		"minecraft:breathable": {
		  "total_supply": 15,
		  "suffocate_time": -1,
		  "inhale_time": 3.75,
		  "generates_bubbles": false
		},
		"minecraft:nameable": {
		  "always_show": true,
		  "allow_name_tag_renaming": false
		},
		"minecraft:physics": {
		},
		"minecraft:pushable": {
		  "is_pushable": false,
		  "is_pushable_by_piston": true
		},
		"minecraft:insomnia": {
		  "days_until_insomnia": 3
		},
		"minecraft:rideable": {
		  "seat_count": 2,
		  "family_types": [
			  "parrot_tame"
			],
			"pull_in_entities": true,
			"seats": [
				{
					"position": [ 0.4, -0.2, -0.1 ],
					"min_rider_count": 0,
			  		"max_rider_count": 0,
			 		 "lock_rider_rotation": 0
				},
				{
			 		"position": [ -0.4, -0.2, -0.1 ],
					"min_rider_count": 1,
					"max_rider_count": 2,
					"lock_rider_rotation": 0
				}
			]
		},
		"minecraft:conditional_bandwidth_optimization": {
		},
		"minecraft:block_climber": {},
		"minecraft:damage_sensor": {
			"triggers": [
				{
					"on_damage": {
						"filters": {
							"test": "has_damage",
							"subject": "self",
							"value": "fatal"
						},
						"event": "player_died",
						"target": "self"
					},
					"deals_damage": true
				}
			]
		},
		"minecraft:environment_sensor": {
		  "triggers":[
			  {
				  "filters": {
					  "all_of": [
						  {
							  "test": "has_mob_effect",
							  "subject": "self",
							  "value": "bad_omen"
							},
							{
								"test": "is_in_village",
								"subject": "self",
								"value": true
							},
							{
								"test": "has_tag",
								"subject": "self",
								"operator": "!=",
								"value": "01"
							}
						]
					},
					"event": "kcraft:filter_raid_trigger"
				}
			]
		}
	},
	"events": {
		"minecraft:gain_bad_omen": {
		  "add": {
			"component_groups": [
			  "minecraft:add_bad_omen"
			]
		  }
		},
		"minecraft:clear_add_bad_omen": {
		  "remove": {
			"component_groups": [
			  "minecraft:add_bad_omen"
			]
		  },
		  "add": {
			"component_groups": [
			  "minecraft:clear_bad_omen_spell_effect"
			]
		  }
		},
		"kcraft:filter_raid_trigger":{
			"add": {
				"component_groups": [
					"kcraft_raid_trigger_filter"
				]
			}
		},
		"minecraft:trigger_raid": {
		  "add": {
			"component_groups": [ "minecraft:raid_trigger" ]
		  }
		},
		"minecraft:remove_raid_trigger": {
		  "remove": {
			"component_groups": [ "minecraft:raid_trigger","kcraft_raid_trigger_filter" ]
		  }
		},
		"kcraft:add_command_id_1": {
			"add": {
				"component_groups": [
					"kcraft:execute_command_id_1"
				]
			}
		},
		"kcraft:remove_command_id_1": {
			"add": {
				"component_groups": [
					"kcraft:execute_no_command"
				]
			},
			"remove": {
				"component_groups": [
					"kcraft:execute_command_id_1"
				]
			}
		},
		"kcraft:remove_skin_ids":{
			"sequence": [
				{
					"remove": {
						"component_groups": [
							"kcraft:execute_command_id_1"
						]
					}
				},
				{
					"remove": {
						"component_groups": [
							"kcraft:execute_no_command"
						]
					}
				}
			]
		},
		"kcraft:nerf": {
			"add": {
				"component_groups": [
					"kcraft_nerf"
				]
			}
		},
		"kcraft:remove_nerf": {
			"remove": {
				"component_groups": [
					"kcraft_nerf"
				]
			}
		},
		"kcraft:immunize": {
			"add": {
				"component_groups": [
					"immunity"
				]
			}
		},
		"kcraft:remove_immunity":{
		  "remove": {
			"component_groups": [
			  "immunity"
			]
		  }
		},
		"minecraft:exited_hive": {
		},
		"player_died":{}
	}
}
}